<?xml version="1.0" encoding="UTF-8"?>
<ns:schema 
	xmlns:ns="http://www.w3.org/2001/XMLSchema" 
	xmlns:pss="http://www.ieee.org/odm/Schema/2008"
	targetNamespace="http://www.ieee.org/odm/Schema/2008" 
	elementFormDefault="qualified" >

    <ns:include schemaLocation=""></ns:include>
    <ns:include schemaLocation="UnitType.xsd"></ns:include>
	
    <ns:annotation>
		<ns:documentation>Copyright www.interpss.org 2008-2011</ns:documentation>
	</ns:annotation>

    <ns:complexType name="NameTagXmlType">
		<ns:sequence>
			<ns:element name="desc" type="ns:string" minOccurs="0" maxOccurs="1"></ns:element>
		</ns:sequence>
		<ns:attribute name="name" type="ns:string" use="optional"></ns:attribute>
	</ns:complexType>


	<ns:complexType name="BaseRecordXmlType">
		<ns:complexContent>
			<ns:extension base="pss:NameTagXmlType">
				<ns:sequence>
					<ns:element name="isoCode" type="ns:string" minOccurs="0" maxOccurs="1"></ns:element>
					<ns:element name="normalOffLineStatus" type="ns:boolean" minOccurs="0"></ns:element>
					<ns:element name="ownerList" minOccurs="0" maxOccurs="unbounded" type="pss:OwnerXmlType"></ns:element>
					<ns:element name="nvPairList" type="pss:NameValuePairXmlType" minOccurs="0" maxOccurs="unbounded"></ns:element>
					<ns:element name="extension" type="ns:anySimpleType" minOccurs="0"></ns:element>
				</ns:sequence>
				<ns:attribute name="areaNumber" type="ns:int" use="optional"></ns:attribute>
				<ns:attribute name="areaName" type="ns:string" use="optional"></ns:attribute>
				<ns:attribute name="zoneNumber" type="ns:int" use="optional"></ns:attribute>
				<ns:attribute name="zoneName" type="ns:string" use="optional"></ns:attribute>
				<ns:attribute name="number" type="ns:long" use="optional"></ns:attribute>
				<ns:attribute name="offLine" type="ns:boolean" use="optional"></ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>


	<ns:complexType name="OwnerXmlType">
		<ns:attribute name="id" type="ns:string" use="required"></ns:attribute>
		<ns:attribute name="name" type="ns:string" use="optional"></ns:attribute>
		<ns:attribute name="number" type="ns:int" use="optional"></ns:attribute>
		<ns:attribute name="ownership" type="ns:double" use="optional">
			<ns:annotation>
				<ns:documentation>
					ownership in pu</ns:documentation>
			</ns:annotation>
		</ns:attribute>
	</ns:complexType>									

	
    <ns:complexType name="NameValuePairXmlType">
		<ns:sequence>
			<ns:element name="name" type="ns:string" minOccurs="1" maxOccurs="1"></ns:element>
			<ns:element name="value" type="ns:string" minOccurs="1" maxOccurs="1"></ns:element>
		</ns:sequence>
	</ns:complexType>
    

    <ns:complexType name="IDRecordXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseRecordXmlType">
				<ns:attribute name="id" type="ns:ID" use="required"></ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="IDRefRecordXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseRecordXmlType">
				<ns:sequence>
				</ns:sequence>
				<ns:attribute name="idRef" type="ns:IDREF" use="required"></ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>

    
    <ns:complexType name="BusIDRefXmlType">
		<ns:complexContent>
			<ns:extension base="pss:IDRefRecordXmlType">
				<ns:sequence>
				</ns:sequence>
				<ns:attribute name="windingType" type="pss:BusWindingEnumType" use="optional">
					<ns:annotation><ns:documentation>
					optional, for indicating bus xfr winding type (Primary, Secondary or tertiary)
					</ns:documentation></ns:annotation>
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="BusRefXmlType">
    	<ns:complexContent>
    		<ns:extension base="pss:NameTagXmlType">
    			<ns:attribute name="id" type="ns:string" use="required"></ns:attribute>
    		</ns:extension>
    	</ns:complexContent>
    </ns:complexType>


    <ns:complexType name="BranchRefXmlType">
    	<ns:complexContent>
    		<ns:extension base="pss:NameTagXmlType">
				<ns:attribute name="fromBusId" type="ns:string" use="required"></ns:attribute>
				<ns:attribute name="toBusId" type="ns:string" use="required"></ns:attribute>
				<ns:attribute name="tertiaryBusId" type="ns:string" use="optional"></ns:attribute>
				<ns:attribute name="circuitId" type="ns:string" use="required"></ns:attribute> 
			</ns:extension>
    	</ns:complexContent>
    </ns:complexType>
    

    <ns:complexType name="BaseDoubleXmlType">
		<ns:attribute name="value" type="ns:double" use="required"></ns:attribute>
	</ns:complexType>
    

    <ns:complexType name="LimitXmlType">
		<ns:attribute name="max" type="ns:double" use="required"></ns:attribute>
		<ns:attribute name="min" type="ns:double" use="required"></ns:attribute>
		<ns:attribute name="active" type="ns:boolean" use="optional" default="true"></ns:attribute>
	</ns:complexType>
    

    <ns:complexType name="ComplexXmlType">
		<ns:attribute name="re" type="ns:double" use="optional" default="0.0"></ns:attribute>
		<ns:attribute name="im" type="ns:double" use="required"></ns:attribute>
	</ns:complexType>
    

    <ns:complexType name="ZXmlType">
		<ns:complexContent>
			<ns:extension base="pss:ComplexXmlType">
				<ns:attribute name="unit" use="required" type="pss:ZUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="NamedZXmlType">
		<ns:complexContent>
			<ns:extension base="pss:ZXmlType">
				<ns:attribute name="name" use="optional" type="ns:string"></ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="RXmlType">
		<ns:attribute name="r" type="ns:double" use="required"></ns:attribute>
		<ns:attribute name="unit" use="required" type="pss:ZUnitType"></ns:attribute>
	</ns:complexType>
    
	
    <ns:complexType name="XXmlType">
		<ns:attribute name="x" type="ns:double" use="required"></ns:attribute>
		<ns:attribute name="unit" use="required" type="pss:ZUnitType"></ns:attribute>
	</ns:complexType>
    

    <ns:complexType name="YXmlType">
		<ns:complexContent>
			<ns:extension base="pss:ComplexXmlType">
				<ns:attribute name="unit" use="required" type="pss:YUnitType" />
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    
    <ns:complexType name="GXmlType">
		<ns:attribute name="g" type="ns:double" use="required"></ns:attribute>
		<ns:attribute name="unit" use="required" type="pss:YUnitType"></ns:attribute>
	</ns:complexType>
    

    <ns:complexType name="XformerZTableXmlType">
		<ns:sequence>
			<ns:element name="XformerZTableItem" minOccurs="0" maxOccurs="unbounded">
				<ns:complexType>
					<ns:sequence>
						<ns:element name="number" type="ns:int" minOccurs="1"></ns:element>
						<ns:element name="lookup" minOccurs="0" maxOccurs="unbounded">
							<ns:complexType>
								<ns:sequence>
									<ns:element name="turnRatioShiftAngle" type="ns:double" minOccurs="1"></ns:element>
									<ns:element name="scaleFactor" type="ns:double" minOccurs="1"></ns:element>
								</ns:sequence>
							</ns:complexType>
						</ns:element>
					</ns:sequence>
				</ns:complexType>
			</ns:element>
		</ns:sequence>
	</ns:complexType>
    
	
    <ns:complexType name="PowerXmlType">
		<ns:complexContent>
			<ns:extension base="pss:ComplexXmlType">
				<ns:attribute name="unit" use="required" type="pss:ApparentPowerUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    	

    <ns:complexType name="PowerFactorXmlType">
		<ns:complexContent>
			<ns:extension base="pss:FactorXmlType">
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    	
	
    <ns:complexType name="FactorXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:FactorUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="ActivePowerXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:ActivePowerUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="ReactivePowerXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:ReactivePowerUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="TurnRatioXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:TurnRatioUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="VoltageXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" type="pss:VoltageUnitType" use="required"></ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="AngleXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:AngleUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="CurrentXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:CurrentUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="AdjustmentDataXmlType">
		<ns:annotation><ns:documentation>
		There are two adj modes: ValueAdjustment and RangeAdjustment. For ValueAdjustment, the desiredValue is used. For RangeAdjustment,
		max and min are used.
		</ns:documentation></ns:annotation>
		<ns:complexContent>
			<ns:extension base="pss:LimitXmlType">
				<ns:attribute name="mode" use="required" type="pss:AdjustmentModeEnumType"></ns:attribute>
				<ns:attribute name="desiredValue" type="ns:double" use="required"></ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="ScEquivSourceXmlType">
		<ns:sequence>
			<ns:element name="scMva3Phase" type="ns:double" minOccurs="1"></ns:element>
			<ns:element name="xOverR3Phase" type="ns:double" minOccurs="1"></ns:element>
			<ns:element name="scMva1Phase" type="ns:double" minOccurs="0"></ns:element>
			<ns:element name="xOverR1Phase" type="ns:double" minOccurs="0"></ns:element>
		</ns:sequence>
	</ns:complexType>
    

    <ns:simpleType name="AdjustmentModeEnumType">
		<ns:restriction base="ns:string">
			<ns:enumeration value="ValueAdjustment"></ns:enumeration>
			<ns:enumeration value="RangeAdjustment"></ns:enumeration>
		</ns:restriction>
	</ns:simpleType>
    

    <ns:complexType name="LengthXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:LengthUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="FrequencyXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:FrequencyUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    
    <ns:complexType name="TimePeriodXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:TimePeriodUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="ApparentPowerXmlType">
		<ns:complexContent>
			<ns:extension base="pss:BaseDoubleXmlType">
				<ns:attribute name="unit" use="required" type="pss:ApparentPowerUnitType">
				</ns:attribute>
			</ns:extension>
		</ns:complexContent>
	</ns:complexType>
    

    <ns:complexType name="TransformerZeroSeqXmlType">
		<ns:sequence>
			<ns:element name="conectionLocation" maxOccurs="1" minOccurs="1" type="pss:XfrZeroSeqConnectLocationEnumType">
			</ns:element>
			<ns:element name="zG" type="pss:ZXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="z1" type="pss:ZXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="z2" type="pss:ZXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="z3" type="pss:ZXmlType" maxOccurs="1" minOccurs="0"></ns:element>
		</ns:sequence>
	</ns:complexType>
    

    <ns:simpleType name="XfrZeroSeqConnectLocationEnumType">
		<ns:restriction base="ns:string">
			<ns:enumeration value="AtBus1"></ns:enumeration>
			<ns:enumeration value="AtBus2"></ns:enumeration>
			<ns:enumeration value="BetweenBus1nBus2"></ns:enumeration>
		</ns:restriction>
	</ns:simpleType>
    
	
    <ns:complexType name="MutualZeroZXmlType">
		<ns:sequence>
			<ns:element name="branchFromBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="1"></ns:element>
			<ns:element name="branchToBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="1"></ns:element>
			<ns:element name="branchCirId" type="ns:string" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="zM" type="pss:ZXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="bIJ1pu" type="ns:double" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="bIJ2pu" type="ns:double" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="bKL1pu" type="ns:double" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="bKL2pu" type="ns:double" maxOccurs="1" minOccurs="0"></ns:element>
		</ns:sequence>
	</ns:complexType>
    	

    <ns:complexType name="AcscFaultXmlType">
		<ns:sequence>
			<ns:element name="faultType" minOccurs="1" type="pss:AcscFaultTypeEnumType" />
			<ns:element name="faultCategory" minOccurs="1" type="pss:AcscFaultCategoryEnumType" />
			<ns:element name="refBusBranch" type="pss:IDRefRecordXmlType" minOccurs="1" />
			<ns:element name="zLG" type="pss:ZXmlType" maxOccurs="1" minOccurs="0" />
			<ns:element name="zLL" type="pss:ZXmlType" maxOccurs="1" minOccurs="0" />
			<ns:element name="distance" type="ns:double" maxOccurs="1" minOccurs="0" />
			<ns:element name="branchReclosure" type="ns:boolean" maxOccurs="1" minOccurs="0" />
			<ns:element name="reclosureTime" type="pss:TimePeriodXmlType" minOccurs="0" maxOccurs="1" />
		</ns:sequence>
	</ns:complexType>
    

    <ns:complexType name="XformerConnectionXmlType">
		<ns:sequence>
			<ns:element name="grounding" type="pss:GroundingXmlType" minOccurs="0"></ns:element>
		</ns:sequence>
		<ns:attribute name="xfrConnection" type="pss:XformrtConnectionEnumType" use="required"></ns:attribute>
	</ns:complexType>
    
	
    <ns:complexType name="GroundingXmlType">
		<ns:sequence>
			<ns:element name="groundingZ" type="pss:ZXmlType" minOccurs="0"></ns:element>
		</ns:sequence>
		<ns:attribute name="groundingConnection" type="pss:GroundingEnumType" use="required"></ns:attribute>
	</ns:complexType>
    

    <ns:simpleType name="ShortCircuitBusEnumType">
		<ns:restriction base="ns:string">
			<ns:enumeration value="Contributing"></ns:enumeration>
			<ns:enumeration value="Non-contributing"></ns:enumeration>
		</ns:restriction>
	</ns:simpleType>
    

    <ns:simpleType name="XformrtConnectionEnumType">
		<ns:restriction base="ns:string">
			<ns:enumeration value="Wye"></ns:enumeration>
			<ns:enumeration value="Delta"></ns:enumeration>
		</ns:restriction>
	</ns:simpleType>
    

    <ns:simpleType name="AcscFaultCategoryEnumType">
		<ns:restriction base="ns:string">
			<ns:enumeration value="Fault3Phase"></ns:enumeration>
			<ns:enumeration value="LineToLine"></ns:enumeration>
			<ns:enumeration value="LineToGround"></ns:enumeration>
			<ns:enumeration value="LineLineToGround"></ns:enumeration>
			<ns:enumeration value="Outage_3Phase"></ns:enumeration>
			<ns:enumeration value="Outage_1Phase"></ns:enumeration>
			<ns:enumeration value="Outage_2Phase"></ns:enumeration>			
		</ns:restriction>
	</ns:simpleType>
    

    <ns:simpleType name="AcscFaultTypeEnumType">
		<ns:restriction base="ns:string">
			<ns:enumeration value="BusFault"></ns:enumeration>
			<ns:enumeration value="BranchFault"></ns:enumeration>
			<ns:enumeration value="BranchOutage"></ns:enumeration>
		</ns:restriction>
	</ns:simpleType>
    

    <ns:complexType name="BranchFaultXmlType">
		<ns:sequence>
			<ns:element name="faultCategory" type="pss:AcscFaultCategoryEnumType" maxOccurs="1" minOccurs="1"></ns:element>
			<ns:element name="fromBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="1"></ns:element>
			<ns:element name="toBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="faultedCirId" type="ns:string" maxOccurs="unbounded" minOccurs="0"></ns:element>
			<ns:element name="fromBusRatedV" type="pss:VoltageXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="toBusRatedV" type="pss:VoltageXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="faultStartTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="firstOperationTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="secondOperationTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="faultDurationTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="faultZ" type="pss:ZXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="faultLocationFromFromSide" type="ns:double" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="permanentFault" type="ns:boolean" maxOccurs="1" minOccurs="0"></ns:element>
		</ns:sequence>
	</ns:complexType>
    

    <ns:complexType name="BusFaultXmlType">
		<ns:sequence>
			<ns:element name="faultCategory" type="pss:AcscFaultCategoryEnumType" maxOccurs="1" minOccurs="1"></ns:element>
			<ns:element name="faultedBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="1"></ns:element>
			<ns:element name="faultedBusRatedV" type="pss:VoltageXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="remoteEndBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="remoteEndBusRatedV" type="pss:VoltageXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="faultStartTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="firstOperationTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="secondOperationTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="faultDurationTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="faultZ" type="pss:ZXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="permanentFault" type="ns:boolean" maxOccurs="1" minOccurs="0"></ns:element>
		</ns:sequence>
	</ns:complexType>
    

    <ns:complexType name="BranchOutageXmlType">
		<ns:sequence>
			<ns:element name="faultCategory" type="pss:AcscFaultCategoryEnumType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="fromBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="1"></ns:element>
			<ns:element name="toBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="1"></ns:element>
			<ns:element name="cirId" type="ns:string" minOccurs="0" maxOccurs="1"></ns:element>
			<ns:element name="startTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="1"></ns:element>
		</ns:sequence>
	</ns:complexType>
    

    <ns:complexType name="LoadChangeDynamicEventXmlType">
		<ns:sequence>
			<ns:element name="loadChangeCategory" maxOccurs="1" minOccurs="0" type="pss:LoadChangeCategoryEnumType"></ns:element>
			<ns:element name="bus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="busRatedVoltage" type="pss:VoltageXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="operationTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="operateAtFrequence" type="pss:FrequencyXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="operateAtVoltage" type="pss:VoltageXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="operationDelayTime" type="pss:TimePeriodXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="constantPChange" type="pss:PowerXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="constantIChange" type="pss:PowerXmlType" maxOccurs="1" minOccurs="0"></ns:element>
			<ns:element name="constantZChange" type="pss:PowerXmlType" maxOccurs="1" minOccurs="0"></ns:element>
		</ns:sequence>
	</ns:complexType>
    

    <ns:simpleType name="LoadChangeCategoryEnumType">
		<ns:restriction base="ns:string">
			<ns:enumeration value="LowFrequence"></ns:enumeration>
			<ns:enumeration value="LowVoltage"></ns:enumeration>
