<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
         xmlns:p="http://www.springframework.org/schema/p"
	     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	     xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="sparseEqnInteger" 
	      class="com.interpss.core.sparse.impl.SparseEqnIntegerImpl"
	      scope="singleton">	
	</bean>

<!--
	Default Double Sparse Eqn implementation   
-->
	<bean id="sparseEqnDouble" 
	      class="com.interpss.core.sparse.impl.SparseEqnDoubleImpl"
	      scope="singleton">	
	</bean>
	<bean id="sparseEqnDoubleDev" 
	      class="com.interpss.core.sparse.dev.SparseEqnDoubleDevImpl"
	      scope="singleton">	
	</bean>
	<!-- bean id="sparseEqnDoubleNative" 
	      class="com.interpss.numeric.sparse.SparseEqnDoubleNativeImpl"
	      scope="singleton">	
	</bean -->
<!--
	An alternative Double Sparse Eqn implementation   
-->
	<bean id="sparseEqnDoubleCommonMath" 
	      class="org.interpss.numeric.sparse.impl.SparseEqnDoubleCommonMathImpl"
	      scope="singleton">	
	</bean>

	<bean id="sparseEqnMatrix2x2" 
	      class="com.interpss.core.sparse.impl.SparseEqnMatrix2x2Impl"
	      scope="singleton">	
	</bean>

	<bean id="sparseEqnMatrix2x2Dev" 
	      class="com.interpss.core.sparse.dev.SparseEqnMatrix2x2DevImpl"
	      scope="singleton">	
	</bean>

	<bean id="sparseEqnComplex" 
	      class="com.interpss.core.sparse.impl.SparseEqnComplexImpl"
	      scope="singleton">	
	</bean>
</beans>
