<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
   targetNamespace="http://www.ieee.org/odm/Schema/2008" 
   xmlns:pss="http://www.ieee.org/odm/Schema/2008" 
   xmlns:pss.unit="http://www.ieee.org/odm/Schema/2008/unit#" 
   elementFormDefault="qualified">

	<annotation>
		<documentation>Copyright www.interpss.org 2008-2011</documentation>
	</annotation>
<!-- 
	<simpleType name="ApparentPowerUnitType">
		<restriction base="anyURI">
			<enumeration value="pss.unit:ApparentPower.PU"></enumeration>
			<enumeration value="pss.unit:ApparentPower.VA"></enumeration>
			<enumeration value="pss.unit:ApparentPower.KVA"></enumeration>
			<enumeration value="pss.unit:ApparentPower.MVA"></enumeration>
		</restriction>
	</simpleType>
 -->	

	<simpleType name="ApparentPowerUnitType">
		<restriction base="string">
			<enumeration value="PU"></enumeration>
			<enumeration value="VA"></enumeration>
			<enumeration value="KVA"></enumeration>
			<enumeration value="MVA"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="ActivePowerUnitType">
		<restriction base="string">
			<enumeration value="PU"></enumeration>
			<enumeration value="W"></enumeration>
			<enumeration value="KW"></enumeration>
			<enumeration value="MW"></enumeration>
			<enumeration value="HP"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="ReactivePowerUnitType">
		<restriction base="string">
			<enumeration value="PU"></enumeration>
			<enumeration value="VAR"></enumeration>
			<enumeration value="KVAR"></enumeration>
			<enumeration value="MVAR"></enumeration>
			<enumeration value="Ohm"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="FactorUnitType">
		<restriction base="string">
			<enumeration value="PU"></enumeration>
			<enumeration value="Percent"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="TurnRatioUnitType">
		<restriction base="pss:FactorUnitType">
		</restriction>
	</simpleType>

	<simpleType name="YUnitType">
		<restriction base="string">
			<enumeration value="PU"></enumeration>
			<enumeration value="VAR"></enumeration>
			<enumeration value="KVAR"></enumeration>
			<enumeration value="MVAR"></enumeration>
			<enumeration value="MHO"></enumeration>
			<enumeration value="MICROMHO"></enumeration>
			<enumeration value="MHOPerMile"></enumeration>
			<enumeration value="MHOPerFt"></enumeration>
			<enumeration value="MHOPerKM"></enumeration>
			<enumeration value="MHOPerM"></enumeration>
			<enumeration value="MICROMHOPerMile"></enumeration>
			<enumeration value="MICROMHOPerFt"></enumeration>
			<enumeration value="MICROMHOPerM"></enumeration>
			<enumeration value="MICROMHOPerKM"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="ZUnitType">
		<restriction base="string">
			<enumeration value="PU"></enumeration>
			<enumeration value="VA"></enumeration>
			<enumeration value="KVA"></enumeration>
			<enumeration value="MVA"></enumeration>
			<enumeration value="OHM"></enumeration>
			<enumeration value="Percent"></enumeration>
			<enumeration value="OHMPerMile"></enumeration>
			<enumeration value="OHMPerFt"></enumeration>
			<enumeration value="OHMPerKM"></enumeration>
			<enumeration value="OHMPerM"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="AngleUnitType">
		<restriction base="string">
			<enumeration value="RAD"></enumeration>
			<enumeration value="DEG"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="LengthUnitType">
		<restriction base="string">
			<enumeration value="M"></enumeration>
			<enumeration value="Ft"></enumeration>
			<enumeration value="KM"></enumeration>
			<enumeration value="Mile"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="FrequencyUnitType">
		<restriction base="string">
			<enumeration value="Hz"></enumeration>
			<enumeration value="KHz"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="TimePeriodUnitType">
		<restriction base="string">
			<enumeration value="Sec"></enumeration>
			<enumeration value="PU"></enumeration>
			<enumeration value="Min"></enumeration>
			<enumeration value="Hour"></enumeration>
			<enumeration value="Cycle"></enumeration>
		</restriction>
	</simpleType>
	<simpleType name="CurrentUnitType">
		<restriction base="string">
			<enumeration value="PU"></enumeration>
			<enumeration value="AMP"></enumeration>
			<enumeration value="KA"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="VoltageUnitType">
		<restriction base="string">
			<enumeration value="PU"></enumeration>
			<enumeration value="VOLT"></enumeration>
			<enumeration value="KV"></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="TapUnitType">
		<restriction base="pss:TurnRatioUnitType"></restriction>
	</simpleType>
</schema>