			<element maxOccurs="1" minOccurs="1" name="T3" type="pss:TimePeriodXmlType"/>			
			<element maxOccurs="1" minOccurs="1" name="PMAX" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="PMIN" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="Pup" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="Pdown" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="TCH" type="pss:TimePeriodXmlType"/>			
		</sequence>
		</extension>
		</complexContent>		
	</complexType>
	
	<complexType name="GovSteamTCSRXmlType">
		<complexContent>
		<extension base="pss:GovSteamNRXmlType">
		<sequence>			
			<element maxOccurs="1" minOccurs="1" name="TRH" type="pss:TimePeriodXmlType"/>
			<element maxOccurs="1" minOccurs="1" name="TCO" type="pss:TimePeriodXmlType"/>						
			<element maxOccurs="1" minOccurs="1" name="FCH" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="FIP" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="FLP" type="double"/>						
		</sequence>
		</extension>
		</complexContent>		
	</complexType>
	
	<complexType name="GovSteamTDSRXmlType">
		<complexContent>
		<extension base="pss:GovSteamNRXmlType">
		<sequence>			
			<element maxOccurs="1" minOccurs="1" name="TRH1" type="pss:TimePeriodXmlType"/>
			<element maxOccurs="1" minOccurs="1" name="TRH2" type="pss:TimePeriodXmlType"/>
			<element maxOccurs="1" minOccurs="1" name="TCO" type="pss:TimePeriodXmlType"/>	
			<element maxOccurs="1" minOccurs="1" name="FVHP" type="double"/>					
			<element maxOccurs="1" minOccurs="1" name="FHP" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="FIP" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="FLP" type="double"/>						
		</sequence>
		</extension>
		</complexContent>		
	</complexType>

