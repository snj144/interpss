			<element maxOccurs="1" minOccurs="1" name="KS3" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="T8" type="pss:TimePeriodXmlType"/>
			<element maxOccurs="1" minOccurs="1" name="T9" type="pss:TimePeriodXmlType"/>
			<element maxOccurs="1" minOccurs="1" name="KS1" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="T1" type="pss:TimePeriodXmlType"/>
			<element maxOccurs="1" minOccurs="1" name="T2" type="pss:TimePeriodXmlType"/>
			<element maxOccurs="1" minOccurs="1" name="T3" type="pss:TimePeriodXmlType"/>
			<element maxOccurs="1" minOccurs="1" name="T4" type="pss:TimePeriodXmlType"/>
			<element maxOccurs="1" minOccurs="1" name="VSTMAX" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="VSTMIN" type="double"/>
		</sequence>
		</extension>
		</complexContent>
	</complexType>

