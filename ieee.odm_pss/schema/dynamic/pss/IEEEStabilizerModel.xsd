			<element maxOccurs="1" minOccurs="1" name="T6" type="pss:TimePeriodXmlType"/>			
			<element maxOccurs="1" minOccurs="1" name="VSTMAX" type="double"/>
			<element maxOccurs="1" minOccurs="1" name="VSTMIN" type="double"/>
			<element maxOccurs="1" minOccurs="0" name="A1" type="double"/>
			<element maxOccurs="1" minOccurs="0" name="A2" type="double"/>	
		</sequence>
		</extension>
		</complexContent>		
	</complexType>

