			<element maxOccurs="1" minOccurs="0" name="VCL" type="double"/>
			<element maxOccurs="1" minOccurs="0" name="VCUTOFF" type="double"/>
		</sequence>
		</extension>
		</complexContent>		
	</complexType>
	
