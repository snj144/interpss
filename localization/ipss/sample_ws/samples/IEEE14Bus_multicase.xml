<ipss:InterPSS xmlns:ipss="http://www.interpss.org/schema"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ipss:runStudyCase>
		<ipss:analysisRunType>RunAclf</ipss:analysisRunType>
		<ipss:gridRun>
			<ipss:enableGridRun>true</ipss:enableGridRun>
			<ipss:remoteJobCreation>true</ipss:remoteJobCreation>
			<ipss:aclfOption>
				<ipss:returnOnlyViolationCase>true</ipss:returnOnlyViolationCase>
				<ipss:calBranchLimitViolation>true</ipss:calBranchLimitViolation>
				<ipss:calBusVoltageViolation>true</ipss:calBusVoltageViolation>
				<ipss:busVoltagePULimit>
					<ipss:max>1.2</ipss:max>
					<ipss:min>0.8</ipss:min>
				</ipss:busVoltagePULimit>
			</ipss:aclfOption>
		</ipss:gridRun>
		<ipss:runAclfStudyCase>
		   <ipss:defaultAclfAlgorithm>
			   <ipss:lfMethod>NR</ipss:lfMethod>
  			   <ipss:maxIterations>20</ipss:maxIterations>
			   <ipss:tolerance>10.0</ipss:tolerance>
			   <ipss:toleranceUnit>KVA</ipss:toleranceUnit>
			   <ipss:initBusVoltage>true</ipss:initBusVoltage>
			   <ipss:diaplaySummary>true</ipss:diaplaySummary>
			   <ipss:accFactor>0.0</ipss:accFactor>
		   </ipss:defaultAclfAlgorithm>		
		   <ipss:aclfStudyCaseList>
			 <ipss:aclfStudyCase>
				<ipss:recId>breakline_2</ipss:recId>
			</ipss:aclfStudyCase>
			
			<ipss:aclfStudyCase>
				<ipss:recId>breakline_3</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_4</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_5</ipss:recId>
			</ipss:aclfStudyCase>
			
			<ipss:aclfStudyCase>
				<ipss:recId>breakline_6</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_7</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_8</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_9</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_10</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_11</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_12</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_13</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
				<ipss:recId>breakline_14</ipss:recId>
			</ipss:aclfStudyCase>

			<ipss:aclfStudyCase>
