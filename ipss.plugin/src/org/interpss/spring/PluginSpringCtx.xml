	      scope="singleton">	
	</bean>		
<!--
		ipss xml to Algorithm mapping 
 -->	
	<bean id="xml2LfAlgorithmMapper" 
	      class="org.interpss.mapper.runCase.XmlCaseData2LfAlgorithmMapperImpl"
	      scope="singleton">	
	      <constructor-arg>
	  			<ref bean="ipssMsgHub"/>
	  	  </constructor-arg>	      
	  	<!-- property name="msg">
	  		<ref bean="ipssMsgHub"/></property -->
	</bean>	

